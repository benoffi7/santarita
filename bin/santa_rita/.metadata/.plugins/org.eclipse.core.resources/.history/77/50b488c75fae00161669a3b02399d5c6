	Panel_Items panel_centro = new Panel_Items();
	add(panel_centro);

	Panel_Items panel_izquierdo = new Panel_Items();
	add(panel_izquierdo);
	setVisible(true);
    }

    public static void cargar_Panel_Derecho()
    {

    }
}
