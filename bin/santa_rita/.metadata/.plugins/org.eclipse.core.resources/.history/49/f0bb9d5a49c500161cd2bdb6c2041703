package componentes;

import javax.swing.JComboBox;

public class Combo_Adicionales extends JComboBox<String>
{
    private String[] data = { "Solo", "Salsa mixta", "Salsa de verdeo" };;

    public Combo_Adicionales()
    {
	super(data);
    }

    public String getSelected()
    {
	return getSelected();
    }
}
